/* pages/index/index.wxss */
.swiper{
    height: 340rpx;
  }
.swiper image{
width: 100%;
}
.navList{
  display: flex;
}
.navList navigator{
  flex: 1;
  padding: 15rpx;
}
.navList image{
  width: 100%;
}
.looplistimg{
  overflow: hidden;
}
.looplistimg navigator{
  box-sizing: border-box;
  float: left;
  width: 33.3%;
}
.looplistimg navigator:nth-child(2),
.looplistimg navigator:nth-child(3){
  border-bottom: 10rpx solid white;
}
.looplistimg navigator:nth-last-child(-n+4){
  height: 27.72711207vw;
  border-left: 10rpx solid white;
}
.looplistimg image{
  width: 100%;
  height: 100%;
}
.looplisttitle image{
  padding: 10rpx 0;
}